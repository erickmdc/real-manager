{
  "name": "real-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:dev": "webpack --entry ./src/app/index.js --output ./public/scripts/app.js",
    "build:prod": "webpack -p --mode=production --verbose --optimize-minimize",
    "start": "webpack-dev-server --open --mode=development",
    "server": "nodemon src/api/app.js --exec babel-node",
    "db": "babel-node src/api/database/config.js",
    "build": "webpack --mode production",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "4.0.0",
    "@material-ui/icons": "4.2.1",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase-admin": "^8.2.0",
    "firebase-functions": "^3.0.2",
    "firebase-tools": "^7.0.2",
    "mini-css-extract-plugin": "^0.7.0",
    "moment": "^2.24.0",
    "mongoose": "^5.6.3",
    "nodemon": "^1.19.1",
    "notistack": "^0.8.8",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.4.1",
    "react-number-format": "^4.0.5",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "uuid": "^3.3.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  }
}
